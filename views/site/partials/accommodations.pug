.locations
    each local in locals
        .map-info(id=local.id class='map-hide')
            .map-info-close
            .map-info-body
                .map-info-title
                    h4= local.title
                .map-info-content
                    p= local.info
                    .map-btns
                        a(class='map-info-btn' href=local.directions target='_blank') Directions
                        a(class='map-info-btn' href=local.address target='_blank') Web Site    
